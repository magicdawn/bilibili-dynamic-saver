{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "ts-node": {
    "swc": true,
    "esm": true
  }
}
